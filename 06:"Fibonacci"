#include <iostream>
using namespace std;

int main() 
{
    int a = 0, b = 1, c, i, n;

    cout << "Ingrese n :";
    cin >> n;

    if (n < 0) 
    {   cout << "Error n debe ser un nÃºmero entero positivo" << endl;
        return 1;
    }

        cout << "Secuencia de Fibonacci: ";
        for (i = 0; i <= n; i++) 
    {   cout << a;
        if (i < n) cout << ", ";
        c = a + b;
        a = b;
        b = c;
    }
    cout << endl;

    return 0;
}
